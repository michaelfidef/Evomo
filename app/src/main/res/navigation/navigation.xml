<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@+id/navigation_home">
    <fragment
        android:id="@+id/navigation_home"
        android:name="com.bangkit.evomo.main.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home"/>
    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.bangkit.evomo.main.history.HistoryFragment"
        android:label="@string/title_history"
        tools:layout="@layout/fragment_history"/>
    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.bangkit.evomo.main.operator.OperatorFragment"
        android:label="@string/title_profile"
        tools:layout="@layout/fragment_profile"/>
    <fragment
        android:id="@+id/navigation_operator"
        android:name="com.bangkit.evomo.main.profile.ProfileFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_operator"/>
</navigation>